{
  "version": 2,
  "builds": [
    {
      "src": "*.js",  // Entry point of your backend
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",  // API routes to handle requests like /api/*
      "dest": "/"  // Destination of the handler (adjust based on your file structure)
    }
  ]
}
